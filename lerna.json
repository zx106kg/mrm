{
  "$schema": "node_modules/lerna/schemas/lerna-schema.json",
  "version": "independent",
  "npmClient": "pnpm",
  "command": {
    "version": {
      "allowBranch": "release",
      "conventionalCommits": true,
      "noPrivate": true,
      "message": "chore: Publish :shipit: [skip ci]"
    }
  },
  "ignoreChanges": [
    "pnpm-lock.json",
    "package-lock.json",
    "Readme.md",
    "Changelog.md",
    "CHANGELOG.md",
    "*.spec.js",
    "*.snap"
  ]
}
